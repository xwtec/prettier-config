{
  "name": "@xwtec/prettier-config",
  "description": "Prettier Configuration for xwtec projects",
  "version": "1.0.2",
  "main": "lib/index.js",
  "repository": "xwtec/prettier-config",
  "author": {
    "name": "xwtec",
    "url": "https://github.com/xwtec"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@fisker/commitlint-config": "^1.0.9",
    "@fisker/lint-staged-config": "^1.0.4",
    "@xwtec/eslint-config": "^6.0.6",
    "@xwtec/prettier-config": "^1.0.2",
    "ava": "^2.0.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "esm": "^3.2.25",
    "husky": "^2.4.0",
    "lint-staged": "^8.2.0",
    "markdownlint-cli": "^0.16.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.3"
  },
  "peerDependencies": {
    "prettier": "*"
  },
  "ava": {
    "require": "esm"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "build:rules": "node -r esm ./scripts/generate-options.js",
    "test": "run-p test:*",
    "test:js": "ava",
    "clean": "rimraf lib",
    "lint": "eslint src/**/*.js --fix",
    "dist": "np --yolo --no-yarn",
    "release": "run-s clean lint build test prettier dist",
    "prettier": "prettier **/*.{js,md} --write"
  },
  "files": [
    "lib"
  ],
  "prettier": "@xwtec/prettier-config"
}
